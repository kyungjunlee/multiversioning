set datafile separator ","
set key autotitle columnhead
set term png size 1200, 250*(num_graphs/2 + num_graphs%2)
set output "throughput_by_batch_size"
set size 9.0, 9.0
set multiplot layout (num_graphs / 2) + (num_graphs %2), 2
do for [i=0:num_graphs-1] {
  stats 'data_tmp'.i every ::0::1 using 3 nooutput
  scheds = STATS_min
  stats 'data_tmp'.i every ::0::1 using 4 nooutput
  execs = STATS_min
  set title sprintf("Sched threads: %d, Exec threads %d", scheds, execs)
  set ylabel "execution time [ms]"
  set xlabel "batch size"

  plot 'data_tmp'.i using 2:10
}
